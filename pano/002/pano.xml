<krpano version="1.18">
	
	<!-- the skin -->
	<include url="skin/defaultskin.xml" />
	
	<!-- view settings -->
	<view hlookat="0" vlookat="0" maxpixelzoom="1.0" fovmax="150" limitview="range" vlookatmin="-34.722" vlookatmax="34.722" />
	

	<preview url="pano.tiles/preview.jpg" />

	<image type="CUBE" multires="true" tilesize="512">
		<level tiledimagewidth="6400" tiledimageheight="6400">
			<cube url="pano.tiles/mres_%s/l4/%v/l4_%s_%v_%h.jpg" />
		</level>
		<level tiledimagewidth="3200" tiledimageheight="3200">
			<cube url="pano.tiles/mres_%s/l3/%v/l3_%s_%v_%h.jpg" />
		</level>
		<level tiledimagewidth="1536" tiledimageheight="1536">
			<cube url="pano.tiles/mres_%s/l2/%v/l2_%s_%v_%h.jpg" />
		</level>
		<level tiledimagewidth="768" tiledimageheight="768">
			<cube url="pano.tiles/mres_%s/l1/%v/l1_%s_%v_%h.jpg" />
		</level>

		<mobile>
			<cube url="pano.tiles/mobile_%s.jpg" />
		</mobile>
	</image>

</krpano>
